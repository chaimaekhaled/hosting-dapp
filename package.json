{
  "name": "hosting-dapp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.1.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "reactstrap": "^5.0.0",
    "web3": "^1.0.0-beta.34"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "compile": "truffle compile && npm run migrate",
    "migrate": "truffle migrate --reset && npm run contracts:sync-src",
    "contracts:sync-src": "rimraf src/contracts && cp -R build/contracts/ src"
  },
  "devDependencies": {
    "detect-port": "^1.2.3",
    "eslint": "^4.19.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-react": "^7.8.2",
    "path-exists": "^3.0.0",
    "react-dev-utils": "^5.0.1",
    "truffle-contract": "^3.0.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
